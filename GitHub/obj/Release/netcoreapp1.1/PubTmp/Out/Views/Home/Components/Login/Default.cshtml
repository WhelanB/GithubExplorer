@model Octokit.User


    @if(Model == null)
    {
        <li><a asp-area="" asp-controller="Home" asp-action="Login" >Login</a></li>
    }
    else
    {
        <li><a asp-area="" asp-controller="Home" asp-action="Index">Graph</a></li>
        <li><a asp-area="" asp-controller="Home" asp-action="Profile">@Model.Name</a></li>
        <li><a asp-area="" asp-controller="Home" asp-action="SignOut">Sign Out</a></li>
    }